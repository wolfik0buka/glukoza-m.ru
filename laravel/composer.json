{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "5.1.*",
        "guzzlehttp/guzzle": "^6.2",
        "ixudra/curl": "^6.7",
        "sentry/sentry-laravel": "^1.0",
        "doctrine/dbal": "^2.5",
        "orchestra/parser": "~3.0",
        "nathanmac/Parser": "^4.3",
        "openlss/lib-array2xml": "^0.5.1",
        "thomaswelton/laravel-mcrypt-faker": "1.0",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "argentcrusade/flysystem-selectel": "^1.1",
        "intervention/image": "^2.4",
        "voronkovich/sberbank-acquiring-client": "^2.3",
        "sanmai/cdek-sdk": "^0.6.25"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "barryvdh/laravel-ide-helper": "v2.2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
